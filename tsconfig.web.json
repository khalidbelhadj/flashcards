{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/*.d.ts",
    "src/lib/**/*"
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "composite": true,
    "paths": {
      "@/*": [
        "src/renderer/src/*"
      ]
    }
  }
}
